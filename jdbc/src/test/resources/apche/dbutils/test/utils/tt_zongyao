
  CREATE TABLE "TT"."ZY" 
   (	"ID" VARCHAR2(20 BYTE), 
	"NAME" VARCHAR2(20 CHAR), 
	"YX" NVARCHAR2(100), 
	"NAME_BM" VARCHAR2(50 CHAR), 
	"NAME_LDM" VARCHAR2(20 CHAR), 
	"NAME_EMF" VARCHAR2(20 CHAR), 
	"NAME_MMZ" VARCHAR2(20 CHAR), 
	"DX" VARCHAR2(40 CHAR), 
	"SZHJ" VARCHAR2(200 CHAR), 
	"STXX" VARCHAR2(200 BYTE), 
	"TR" VARCHAR2(20 CHAR), 
	"HB" VARCHAR2(20 CHAR), 
	"HQ" VARCHAR2(20 CHAR), 
	"GQ" VARCHAR2(20 CHAR), 
	"CSQ" VARCHAR2(20 CHAR), 
	"RJ" VARCHAR2(100 CHAR), 
	"QW" VARCHAR2(50 CHAR), 
	"HXCF" VARCHAR2(500 CHAR), 
	"YCJB" VARCHAR2(1000 CHAR), 
	"YYBW" VARCHAR2(50 CHAR), 
	"YYJZ" VARCHAR2(1000 CHAR), 
	"REMARK" CLOB, 
	"TIME_STAR" TIMESTAMP (6) WITH LOCAL TIME ZONE, 
	"TIME_CHANG" TIMESTAMP (6) WITH LOCAL TIME ZONE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  TABLESPACE "USERS" 
 LOB ("REMARK") STORE AS BASICFILE (
  TABLESPACE "USERS" ENABLE STORAGE IN ROW CHUNK 8192 RETENTION 
  NOCACHE LOGGING ) ;
 

   COMMENT ON COLUMN "TT"."ZY"."ID" IS '主键:ID';
 
   COMMENT ON COLUMN "TT"."ZY"."NAME" IS '药材:中文名称';
 
   COMMENT ON COLUMN "TT"."ZY"."YX" IS '药性';
 
   COMMENT ON COLUMN "TT"."ZY"."NAME_BM" IS '药材:别称|别名';
 
   COMMENT ON COLUMN "TT"."ZY"."NAME_LDM" IS '药材:拉丁学名';
 
   COMMENT ON COLUMN "TT"."ZY"."NAME_EMF" IS '药材:二名法';
 
   COMMENT ON COLUMN "TT"."ZY"."NAME_MMZ" IS '命名者';
 
   COMMENT ON COLUMN "TT"."ZY"."DX" IS '毒性';
 
   COMMENT ON COLUMN "TT"."ZY"."SZHJ" IS '生长环境分布';
 
   COMMENT ON COLUMN "TT"."ZY"."STXX" IS '生态习性';
 
   COMMENT ON COLUMN "TT"."ZY"."TR" IS '土壤';
 
   COMMENT ON COLUMN "TT"."ZY"."HB" IS '海拔';
 
   COMMENT ON COLUMN "TT"."ZY"."HQ" IS '花期';
 
   COMMENT ON COLUMN "TT"."ZY"."GQ" IS '果期';
 
   COMMENT ON COLUMN "TT"."ZY"."CSQ" IS '采收期';
 
   COMMENT ON COLUMN "TT"."ZY"."RJ" IS '入经';
 
   COMMENT ON COLUMN "TT"."ZY"."QW" IS '气|味';
 
   COMMENT ON COLUMN "TT"."ZY"."HXCF" IS '化学成分';
 
   COMMENT ON COLUMN "TT"."ZY"."YCJB" IS '药材鉴别';
 
   COMMENT ON COLUMN "TT"."ZY"."YYBW" IS '药用部位';
 
   COMMENT ON COLUMN "TT"."ZY"."YYJZ" IS '药用价值|功效';
 
   COMMENT ON COLUMN "TT"."ZY"."REMARK" IS '说明';
 
   COMMENT ON COLUMN "TT"."ZY"."TIME_STAR" IS '添加时间';
 
   COMMENT ON COLUMN "TT"."ZY"."TIME_CHANG" IS '修改时间';
 
   COMMENT ON TABLE "TT"."ZY"  IS '药材基础信息表';
 
